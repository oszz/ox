<?xml version="1.0" encoding="UTF-8" ?>
<modules xmlns= "http://www.ox.oszz.org/schema"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="http://www.ox.oszz.org/schema xsd/ox-modules-1.0.xsd">  
     
     <!-- 存放模块xml配置的文件目录 -->
     <!-- 可以是classpath目录，也可以是绝对目录 -->
     <xmlPath dirPath="module/xml"/>
     
     <!-- 所有的模块都在这里声明 -->
     <!-- 生成的messageCode按这里模块声明的顺序 进行生成，
     	      尽量不要改动模块顺序，添加新模块，往后追加 -->
     <module name="auth" desc="认证模块" package="org.oszz.ox.server.module.auth" isGenerator="true"
     			xmlFileName="authModule.xml"/>
     			
     <module name="info" desc="信息提示模块" package="org.oszz.ox.server.module.info" isGenerator="true"
     			xmlFileName="infoModule.xml"/>
 
</modules>   
     