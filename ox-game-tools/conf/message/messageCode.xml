<?xml version="1.0" encoding="UTF-8" ?>
<messages xmlns= "http://www.ox.oszz.org/schema"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="http://www.ox.oszz.org/schema ox-messages-1.0.xsd">  
    
    <!-- 按顺序生成协议号，往后追加，尽量不要中间插入 -->
    <!-- 只有type是"CG"的消息，才会有handler处理类 -->
    
    <message name="AuthProto.CGLogin" type="CG" isGenerator="true"
    	paceageName="org.oszz.ox.server.module.auth.msg">
    	<comments>玩家登陆</comments>
    	<handler paceageName="org.oszz.ox.server.module.auth.handler" />
    </message>
    
    <message name="AuthProto.GCLoginInfo" type="GC" isGenerator="true" 
    	paceageName="org.oszz.ox.server.module.auth.msg">
    	<comments>玩家登陆后返回的消息</comments>
    </message>
    
    <message name="AuthProto.CGLogout" type="CG" isGenerator="true" 
    	paceageName="org.oszz.ox.server.module.auth.msg">
    	<comments>玩家主动退出</comments>
    	<handler paceageName="org.oszz.ox.server.module.auth.handler" />
    </message>
</messages>